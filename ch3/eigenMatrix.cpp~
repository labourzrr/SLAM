#include <iostream>
#include <ctime>
using namespace std;

//Eigen 部分
#include <Eigen/Core>
//稠密矩阵的代数运算（逆/特征值等） 
#include <Eigen/Dense>

#define MATRIX_SIZE 50

/*************************
*本程序演示了Eigen基本类型的使用
/*************************/

int main( int argc, char** argv ) 
{
    //Eigen以矩阵为基本数据单元。它是一个模板类。它的前三个参数为：数据类型，行，列
    //声明一个2X3的float矩阵
    Eigen::Matrix<float,2,3> matirx_23;


    //下面是对矩阵的操作
    //输入数据
    matrix_23 << 1,2,3,4,5,6;
    //输出
    cout << matrix_23 << endl;

    return 0;
}
